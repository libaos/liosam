<?xml version="1.0"?>
<sdf version="1.6">
  <world name="pcd_world">
    <include>
      <uri>model://sun</uri>
    </include>
    
    <include>
      <uri>model://ground_plane</uri>
    </include>
    
    <!-- PCD 转换的地形模型 -->
    <include>
      <uri>model://pcd_terrain</uri>
      <pose>0 0 0 0 0 0</pose>
    </include>
    
    <physics type="ode">
      <real_time_update_rate>1000</real_time_update_rate>
      <max_step_size>0.001</max_step_size>
    </physics>
  </world>
</sdf>